<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox minHeight="700.0" minWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hhn.it.pp.javafx.controllers.spellingtrainer.SpellingTrainerServiceController">
   <children>
      <Region prefHeight="10.0" prefWidth="10.0" VBox.vgrow="ALWAYS" />
      <HBox>
         <children>
            <Region prefHeight="10.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
            <StackPane>
               <children>
                  <Label text="Welcome to the SpellingTrainer">
                     <font>
                        <Font size="27.0" />
                     </font>
                     <StackPane.margin>
                        <Insets bottom="50.0" />
                     </StackPane.margin>
                  </Label>
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#handleStartButtonClick" text="Start">
                     <font>
                        <Font size="21.0" />
                     </font>
                     <StackPane.margin>
                        <Insets top="50.0" />
                     </StackPane.margin>
                  </Button>
               </children>
            </StackPane>
            <Region prefHeight="10.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Region prefHeight="10.0" prefWidth="10.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
